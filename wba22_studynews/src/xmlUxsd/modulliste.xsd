<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	
	<xs:element name="modulliste">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="dEintrag" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="kuerzel" type="xs:string" />
							<xs:element name="bezeichnung" type="xs:string" />
						</xs:sequence>
						<xs:attribute name="modulId" type="xs:integer" />	
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

</xs:schema>